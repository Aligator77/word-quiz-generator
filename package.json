    "babel-core": "^6.6.4",
    "babel-eslint": "^5.0.0",
    "babel-plugin-espower": "^2.1.2",
    "babel-plugin-transform-async-to-generator": "^6.4.6",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "isparta": "^4.0.0",
